<% include ../partials/header %>
<h1>Add a book</h1>

<form action="/books" method="POST">
	<div>Name:<input type="text" name="books[name]"></div>
	<div>Author:<input type="text" name="books[author]"></div>
	<div>Year:<input type="text" name="books[year]"></div>
	<div>ISBN:<input type="text" name="books[ISBN]"></div>
	<div>Average Rating:<input type="text" name="books[averageRating]"></div>
	<div>Price:<input type="text" name="books[price]"></div>
	<div><input type="submit"></div>
</form>

